<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(homework) -%>">
  <span class='title'>
    <%= homework.title %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.homeworks_homework_path(homework),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_homeworks_admin_homework_path(homework),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.homeworks_admin_homework_path(homework),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => homework.title),
        :method => :delete %>
  </span>
</li>
